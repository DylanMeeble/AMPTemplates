[
    {
        "ConfigFile": "Zomboid/Server/{{servername}}.ini",
        "AutoMap": true,
        "Importable": true,
        "ConfigType": "kvp",
        "ConfigFormatRegex": "^(?<key>.+?)=(?<value>.*?)$"
    },
    {
        "ConfigFile": "Zomboid/Server/{{servername}}_SandboxVars.lua",
        "AutoMap": true,
        "ConfigType": "kvp",
        "ConfigFormatRegex": "^(?<indent>\\s*)(?<key>[A-Za-z_][A-Za-z0-9_]*)\\s*=\\s*(?<value>[^,{\\r\\n]+)\\s*,\\s*$",
        "ConfigWriteRegex": "^(?<indent>\\s*)(?<key>[A-Za-z_][A-Za-z0-9_]*)\\s*=\\s*(?<value>[^,{\\r\\n]+)\\s*,\\s*$",
        "ConfigWriteFormat": "${indent}${key} = ${value},"
    }
]