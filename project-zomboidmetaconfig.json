[
    {
        "ConfigFile": "Zomboid/Server/{{servername}}.ini",
        "AutoMap": true,
        "Importable": true,
        "ConfigType": "kvp",
        "ConfigFormatRegex": "^(?<key>.+?)=(?<value>.*?)$"
    },
    {
        "ConfigFile": "Zomboid/Server/{{servername}}_SandboxVars.lua",
        "AutoMap": true,
        "Importable": true,
        "ConfigType": "kvp",
        "ConfigFormatRegex": "^\\s*(?<key>[A-Za-z_][A-Za-z0-9_]*)\\s*=\\s*(?<value>[^,{ \\t\\r\\n][^,\\r\\n]*)\\s*,\\s*$",
        "ConfigWriteRegex": "^\\s*(?<prefix>(?<key>[A-Za-z_][A-Za-z0-9_]*)\\s*=\\s*)(?<value>[^,{ \\t\\r\\n][^,\\r\\n]*)(?<suffix>\\s*,\\s*)$",
        "ConfigWriteFormat": "${prefix}${value}${suffix}"
    }
]