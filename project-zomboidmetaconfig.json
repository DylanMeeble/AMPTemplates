[
    {
        "ConfigFile": "Zomboid/Server/{{servername}}.ini",
        "AutoMap": true,
        "Importable": true,
        "ConfigType": "kvp",
        "ConfigFormatRegex": "^(?<key>.+?)=(?<value>.*?)$"
    },
    {
        "ConfigFile": "Zomboid/Server/{{servername}}_SandboxVars.lua",
        "AutoMap": true,
        "Importable": true,
        "ConfigType": "kvp",
        "ConfigFormatRegex": "^\\s*(?<key>[A-Za-z_][A-Za-z0-9_]*)\\s*=\\s*(?<value>[^,{ \\t\\r\\n][^,\\r\\n]*)\\s*,\\s*$",
        "ConfigWriteRegex": "^(?<indent>\\s*)(?<key>[A-Za-z_][A-Za-z0-9_]*)(?<assign>\\s*=\\s*)(?<value>[^,{\\r\\n]+)(?<comma>\\s*,\\s*)$",
        "ConfigWriteFormat": "${indent}${key}${assign}${value}${comma}"
    }
]